{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    ["@babel/preset-react", {
      "runtime": "automatic",
      "importSource": "@emotion/react"
    }]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }],
    ["@emotion/babel-plugin", {
      "sourceMap": true,
      "autoLabel": "dev-only",
      "labelFormat": "[local]"
    }]
  ]
}
